{
    "name": "arcaneflare",
    "version": "0.0.1",
    "license": "UNLICENSED",
    "scripts": {
        "tw-build": "tailwindcss -i ./resources/public/css/input.css -o ./resources/public/css/output.css",
        "tw-watch": "tailwindcss -i ./resources/public/css/input.css -o ./resources/public/css/output.css --watch",
        "clean": "rm -rf .lsp .calva out target resources/public/js node_modules .shadow-cljs .cpcache",
        "dev": "yarn clean; yarn install; concurrently \"yarn tw-watch\" \"npx shadow-cljs watch app\"",
        "build": "yarn clean; yarn install; npx shadow-cljs release app; yarn tw-build",
        "node-test": "yarn clean; yarn install; npx shadow-cljs compile node-test && node out/node-tests.js"
    },
    "devDependencies": {
        "concurrently": "^9.2.0",
        "shadow-cljs": "2.28.9",
        "tailwindcss": "^4.1.11",
        "@tailwindcss/cli": "^4.1.11"
    },
    "dependencies": {
        "@js-joda/core": "3.2.0",
        "@js-joda/locale_en-us": "3.1.1",
        "@js-joda/timezone": "2.5.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
